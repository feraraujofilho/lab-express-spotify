<div class="artist-item row container-fluid">

    {{#each albumsInfo}}
    <div class="col-md-4 col-sm-12 artists-container">
        <img class="test" src="{{images.[0].url}}" onerror="this.src='man-4.jpg'">
        <div class="small-box">
            <p class="album-title">{{name}}</p>
            <a href="/tracks/{{id}}" class="link-album">View Tracks</a>
        </div>
    </div>
    {{/each}}
</div>